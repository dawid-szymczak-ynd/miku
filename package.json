{
  "name": "miku-credit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps --plain",
    "affected:apps:all": "nx affected:apps --plain --all",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --prod",
    "affected:e2e": "nx affected:e2e --prod --headless",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --parallel --maxParallel=5",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "push-images": "ts-node --project ./tools/tsconfig.tools.json ./tools/scripts/push-images.ts",
    "affected-operation": "ts-node --project ./tools/tsconfig.tools.json ./tools/scripts/affected-operation.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/terminus": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@nrwl/angular": "9.5.1",
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@angular/cli": "9.1.0",
    "@nrwl/workspace": "9.5.1",
    "@types/node": "~8.9.4",
    "dotenv": "6.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "eslint": "6.8.0",
    "typescript": "~3.8.3",
    "prettier": "2.0.4",
    "@nrwl/nest": "9.5.1",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@angular-devkit/build-angular": "0.901.0",
    "codelyzer": "~5.0.1",
    "jest-preset-angular": "8.1.2",
    "@nrwl/jest": "9.5.1",
    "jest": "25.2.3",
    "@types/jest": "25.1.4",
    "@types/minimist": "^1.2.0",
    "minimist": "^1.2.5",
    "ng-mocks": "^9.6.3",
    "shallow-render": "^9.0.5",
    "ts-jest": "25.2.1",
    "cypress": "^4.1.0",
    "@nrwl/cypress": "9.5.1",
    "@nrwl/node": "9.5.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "husky": "^4.2.5",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "nx format:check --uncommitted",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "nx format:write --uncommitted && git add CHANGELOG.md"
    }
  }
}
